	file = {Snapshot:/Users/vrlr64/Zotero/storage/4N2IQJX7/s41467-019-14066-5.html:text/html;Full Text PDF:/Users/vrlr64/Zotero/storage/UGUKI58S/Leem et al. - 2020 - Edible unclonable functions.pdf:application/pdf}
