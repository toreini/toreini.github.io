
@article{al-haj_forensics_2019,
	title = {Forensics {Analysis} of {Xbox} {One} {Game} {Console}},
	url = {http://arxiv.org/abs/1904.00734},
	abstract = {Games console devices have been designed to be an entertainment system. However, the 8th generation games console have new features that can support criminal activities and investigators need to be aware of them. This paper highlights the forensics value of the Microsoft game console Xbox One, the latest version of their Xbox series. The Xbox One game console provides many features including web browsing, social networking, and chat functionality. From a forensic perspective, all those features will be a place of interest in forensic examinations. However, the available published literature focused on examining the physical hard drive artefacts, which are encrypted and cannot provide deep analysis of the user's usage of the console. In this paper, we carried out an investigation of the Xbox One games console by using two approaches: a physical investigation of the hard drive to identify the valuable file timestamp information and logical examination via the graphical user interface. Furthermore, this paper identifies potential valuable forensic data sources within the Xbox One and provides best practices guidance for collecting data in a forensically sound manner.},
	urldate = {2020-06-19},
	journal = {arXiv:1904.00734 [cs]},
	author = {Al-Haj, Ali M.},
	month = mar,
	year = {2019},
	note = {arXiv: 1904.00734},
	keywords = {Computer Science - Computers and Society, Computer Science - Cryptography and Security},
