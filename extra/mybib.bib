@inproceedings{li_webletscript_2018,
	title = {{WebletScript}: {A} {Lightweight} {Distributed} {JavaScript} {Engine} for {Internet} of {Things}},
	shorttitle = {{WebletScript}},
	doi = {10.1109/GLOCOM.2018.8647204},
	abstract = {Nowadays, there is no de facto programming language dedicated to the development of complex Internet of Things (IoT) applications containing different kinds of devices. JavaScript has great potential to become the mainstream application-development program language across IoT platforms, as JavaScript is supported by many engines which have different footprint sizes from the KB level to the MB level. However, there are still technical challenges in the coding efficiency and heterogeneous platform supports. In this paper, we propose a lightweight distributed JavaScript engine, WebletScript, to help IoT developers to build applications for a group of heterogeneous IoT devices. WebletScript can run on any device that supports normal C libraries. It enables automatic code slicing and distribution for an IoT application. Therefore, with one-time platform-independent code development, the application can be executed on multiple devices, such as a group of end devices, a smart gateway and the cloud system. Compared to the traditional solution, in which embedded application and cloud application are developed separately, our approach can reduce the lines of code effectively by 50\% and reduce the cost of application update cost by 65\%.},
	booktitle = {2018 {IEEE} {Global} {Communications} {Conference} ({GLOBECOM})},
	author = {Li, Dong and Huang, Bin and Cui, Li and Xu, Zhiwei},
	month = dec,
	year = {2018},
	note = {ISSN: 2576-6813},
	keywords = {Internet of Things, Computer languages, Data structures, Distributed JavaScript engine, Engines, interpreter, IoT platform, Random access memory, Read only memory, Runtime},
	pages = {1--6},
	file = {IEEE Xplore Full Text PDF:/Users/vrlr64/Zotero/storage/V6Q8N7GK/Li et al. - 2018 - WebletScript A Lightweight Distributed JavaScript.pdf:application/pdf;IEEE Xplore Abstract Record:/Users/vrlr64/Zotero/storage/NVVVEE48/8647204.html:text/html}
}

@inproceedings{gascon-samson_smartjs_2017,
	address = {New York, NY, USA},
	series = {{SPLASH} {Companion} 2017},
	title = {{SmartJS}: dynamic and self-adaptable runtime middleware for next-generation {IoT} systems},
	isbn = {978-1-4503-5514-8},
	shorttitle = {{SmartJS}},
	url = {https://doi.org/10.1145/3135932.3135939},
	doi = {10.1145/3135932.3135939},
	abstract = {The Internet of Things (IoT) has gained wide popularity both in the academic and industrial contexts. However, IoT-based systems exhibit many important challenges across many dimensions. In this work, we propose SmartJS, a rich Javascript-based middleware platform and runtime environment that abstracts the complexity of the various IoT platforms by providing a high-level framework for IoT system developers. SmartJS abstracts large-scale distributed system considerations, such as scheduling, monitoring and self-adaptation, and proposes a rich inter-device Javascript-based code migration framework. Finally, it provides debugging and monitoring techniques to analyze performance and observe system-wide security properties.},
	urldate = {2021-04-22},
