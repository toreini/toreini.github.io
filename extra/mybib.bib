	editor = {Kazmierski, Tom J. and Steinhorst, Sebastian and Große, Daniel},
	year = {2020},
	doi = {10.1007/978-3-030-31585-6_3},
	keywords = {Internet, Standards, Black box testing, Cyber-physical systems, Data models, Execution path, Formal verification, Internet of things, IoT platforms, Model-driven development, Safety, Server-client architecture, Software development, System testing, Testing, Thing description, Verification, Web, Web of things, World wide web consortium},
	pages = {47--69},
	file = {Springer Full Text PDF:/Users/vrlr64/Zotero/storage/4IZR9EDC/Korkan et al. - 2020 - Safe Interoperability for Web of Things Devices an.pdf:application/pdf}
