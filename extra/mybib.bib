	month = apr,
	year = {2019},
	note = {Number: 1
Publisher: Nature Publishing Group},
	pages = {1--10},
	file = {Snapshot:/Users/vrlr64/Zotero/storage/8WLD253A/s42005-019-0139-3.html:text/html;Full Text PDF:/Users/vrlr64/Zotero/storage/SP6Q588X/Wali et al. - 2019 - Biological physically unclonable function.pdf:application/pdf}
