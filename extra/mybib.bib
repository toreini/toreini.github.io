	keywords = {Blockchain, contact tracing, COVID-19, cryptography, Cryptography, distributed ledgers, Privacy, Protocols, Public key, smart contracts, vaccine, Vaccines},
	pages = {37936--37950},
	file = {IEEE Xplore Abstract Record:/Users/vrlr64/Zotero/storage/7JJ5BURD/9366740.html:text/html;IEEE Xplore Full Text PDF:/Users/vrlr64/Zotero/storage/53W5R2FN/Ricci et al. - 2021 - Blockchains for COVID-19 Contact Tracing and Vacci.pdf:application/pdf}
}

