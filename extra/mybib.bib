@incollection{desmedt_proofs_1994,
	address = {Berlin, Heidelberg},
	title = {Proofs of {Partial} {Knowledge} and {Simplified} {Design} of {Witness} {Hiding} {Protocols}},
	volume = {839},
	isbn = {978-3-540-58333-2},
	url = {http://link.springer.com/10.1007/3-540-48658-5_19},
	abstract = {Suppose we are given a proof of knowledge P in which a prover demonstrates that he knows a solution to a given problem instance. Suppose also that we have a secret sharing scheme S on n participants. Then under certain assumptions on P and S, we show how to transform P into a witness indistinguishable protocol, in which the prover demonstrates knowledge of the solution to some subset of n problem instances out of a collection of subsets deﬁned by S. For example, using a threshold scheme, the prover can show that he knows at least d out of n solutions without revealing which d instances are involved. If the instances are independently generated, we get a witness hiding protocol, even if P did not have this property. Our results can be used to eﬃciently implement general forms of group oriented identiﬁcation and signatures. Our transformation produces a protocol with the same number of rounds as P and communication complexity n times that of P. Our results use no unproven complexity assumptions.},
	language = {en},
	urldate = {2020-06-19},
	booktitle = {Advances in {Cryptology} — {CRYPTO} ’94},
	publisher = {Springer Berlin Heidelberg},
	author = {Cramer, Ronald and Damgård, Ivan and Schoenmakers, Berry},
